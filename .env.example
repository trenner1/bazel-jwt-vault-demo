# Environment variables for Bazel JWT Vault Demo
# Copy this file to .env and update with your actual values

# Vault Configuration
VAULT_ROOT_TOKEN=your-vault-root-token-here
VAULT_ADDR=http://localhost:8200

# Broker Configuration  
BROKER_URL=http://localhost:8081
ISSUER=http://localhost:8080
AUDIENCE=vault-broker

# Development Team (optional override)
BAZEL_TEAM=team-alpha

# Docker Network (auto-detected by docker-setup.sh)
DOCKER_NETWORK=old_jenkins-vault-poc_default